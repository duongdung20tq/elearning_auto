{
  "manifest_version": 3,
  "name": "FIS-Tự động trả lời",
  "version": "1.0",
  "description": "Tự động chọn đáp án trong bài thi FIS Elearning",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "icons/icons.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "64": "icons/icons.png"
  }
}